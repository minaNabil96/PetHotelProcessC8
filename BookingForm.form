{
  "components": [
    {
      "label": "Имя клиента",
      "type": "textfield",
      "layout": {
        "row": "Row_0",
        "columns": null
      },
      "id": "Field_ClientName",
      "key": "clientName",
      "description": "Полное имя клиента",
      "validate": {
        "required": true,
        "minLength": 2,
        "maxLength": 100
      }
    },
    {
      "label": "Контактный телефон",
      "type": "textfield",
      "layout": {
        "row": "Row_1",
        "columns": null
      },
      "id": "Field_ClientContact",
      "key": "clientContact",
      "description": "Номер телефона для связи",
      "validate": {
        "required": true,
        "pattern": "^[+]?[(]?[0-9]{1,4}[)]?[-\\s./0-9]*$"
      }
    },
    {
      "label": "Кличка питомца",
      "type": "textfield",
      "layout": {
        "row": "Row_2",
        "columns": null
      },
      "id": "Field_PetName",
      "key": "petName",
      "description": "Как зовут вашего питомца",
      "validate": {
        "required": true,
        "minLength": 1,
        "maxLength": 50
      }
    },
    {
      "label": "Вид питомца",
      "type": "select",
      "layout": {
        "row": "Row_3",
        "columns": null
      },
      "id": "Field_PetType",
      "key": "petType",
      "description": "Выберите вид животного",
      "validate": {
        "required": true
      },
      "values": [
        {
          "label": "Собака",
          "value": "dog"
        },
        {
          "label": "Кошка",
          "value": "cat"
        }
      ]
    },
    {
      "label": "Порода",
      "type": "textfield",
      "layout": {
        "row": "Row_4",
        "columns": null
      },
      "id": "Field_PetBreed",
      "key": "petBreed",
      "description": "Порода питомца (опционально)",
      "validate": {
        "required": false
      }
    },
    {
      "label": "Возраст питомца (лет)",
      "type": "number",
      "layout": {
        "row": "Row_5",
        "columns": null
      },
      "id": "Field_PetAge",
      "key": "petAge",
      "description": "Возраст в годах",
      "validate": {
        "required": true,
        "min": 0,
        "max": 30
      }
    },
    {
      "label": "Дата заезда",
      "type": "datetime",
      "subtype": "date",
      "layout": {
        "row": "Row_6",
        "columns": null
      },
      "id": "Field_CheckIn",
      "key": "checkInDate",
      "description": "Дата и время прибытия",
      "dateLabel": "Дата заезда",
      "timeLabel": "Время",
      "validate": {
        "required": true
      }
    },
    {
      "label": "Дата выезда",
      "type": "datetime",
      "subtype": "date",
      "layout": {
        "row": "Row_7",
        "columns": null
      },
      "id": "Field_CheckOut",
      "key": "checkOutDate",
      "description": "Дата и время отъезда",
      "dateLabel": "Дата выезда",
      "timeLabel": "Время",
      "validate": {
        "required": true
      }
    },
    {
      "label": "Особые требования",
      "type": "textarea",
      "layout": {
        "row": "Row_8",
        "columns": null
      },
      "id": "Field_SpecialRequests",
      "key": "specialRequests",
      "description": "Диета, медицинские требования, поведенческие особенности",
      "validate": {
        "required": false,
        "maxLength": 500
      }
    },
    {
      "label": "Темперамент",
      "type": "select",
      "layout": {
        "row": "Row_9",
        "columns": null
      },
      "id": "Field_Temperament",
      "key": "temperament",
      "description": "Характер питомца",
      "validate": {
        "required": true
      },
      "values": [
        {
          "label": "Спокойный",
          "value": "Спокойный"
        },
        {
          "label": "Активный",
          "value": "Активный"
        },
        {
          "label": "Агрессивный",
          "value": "Агрессивный"
        }
      ]
    },
    {
      "label": "Уровень социализации",
      "type": "select",
      "layout": {
        "row": "Row_10",
        "columns": null
      },
      "id": "Field_SocialLevel",
      "key": "socialLevel",
      "description": "Как питомец взаимодействует с другими животными",
      "validate": {
        "required": true
      },
      "values": [
        {
          "label": "Высокий",
          "value": "Высокий"
        },
        {
          "label": "Средний",
          "value": "Средний"
        },
        {
          "label": "Низкий",
          "value": "Низкий"
        }
      ]
    }
  ],
  "type": "default",
  "id": "BookingForm",
  "executionPlatform": "Camunda Cloud",
  "executionPlatformVersion": "8.5.0",
  "exporter": {
    "name": "Camunda Modeler",
    "version": "5.36.1"
  },
  "schemaVersion": 16
}