{
  "schemaVersion": 13,
  "exporter": {
    "name": "Camunda Modeler",
    "version": "5.36.1"
  },
  "components": [
    {
      "text": "## Ветеринарный осмотр",
      "type": "text",
      "id": "Field_Header",
      "layout": {
        "row": "Row_0"
      }
    },
    {
      "label": "Питомец",
      "type": "textfield",
      "id": "Field_PetName",
      "key": "petName",
      "layout": {
        "row": "Row_1"
      },
      "readonly": true
    },
    {
      "label": "Общее состояние",
      "type": "select",
      "id": "Field_GeneralCondition",
      "key": "generalCondition",
      "layout": {
        "row": "Row_2"
      },
      "validate": {
        "required": true
      },
      "values": [
        {
          "label": "Отличное",
          "value": "excellent"
        },
        {
          "label": "Хорошее",
          "value": "good"
        },
        {
          "label": "Удовлетворительное",
          "value": "satisfactory"
        },
        {
          "label": "Требует внимания",
          "value": "needsAttention"
        }
      ]
    },
    {
      "label": "Температура тела (°C)",
      "type": "number",
      "id": "Field_BodyTemperature",
      "key": "bodyTemperature",
      "layout": {
        "row": "Row_3"
      },
      "validate": {
        "min": 35,
        "max": 42
      }
    },
    {
      "label": "Вес (кг)",
      "type": "number",
      "id": "Field_Weight",
      "key": "weight",
      "layout": {
        "row": "Row_4"
      },
      "validate": {
        "required": true
      }
    },
    {
      "label": "Состояние шерсти",
      "type": "select",
      "id": "Field_CoatCondition",
      "key": "coatCondition",
      "layout": {
        "row": "Row_5"
      },
      "values": [
        {
          "label": "Здоровая",
          "value": "healthy"
        },
        {
          "label": "Сухая",
          "value": "dry"
        },
        {
          "label": "Наличие паразитов",
          "value": "parasites"
        }
      ]
    },
    {
      "label": "Обнаружены паразиты?",
      "type": "checkbox",
      "id": "Field_ParasitesFound",
      "key": "parasitesFound",
      "layout": {
        "row": "Row_6"
      }
    },
    {
      "label": "Обнаружены травмы?",
      "type": "checkbox",
      "id": "Field_InjuriesFound",
      "key": "injuriesFound",
      "layout": {
        "row": "Row_7"
      }
    },
    {
      "label": "Описание травм",
      "type": "textarea",
      "id": "Field_InjuryDescription",
      "key": "injuryDescription",
      "layout": {
        "row": "Row_8"
      },
      "conditional": {
        "hide": "=injuriesFound = false"
      }
    },
    {
      "label": "Признаки болезни?",
      "type": "checkbox",
      "id": "Field_IllnessSigns",
      "key": "illnessSigns",
      "layout": {
        "row": "Row_9"
      }
    },
    {
      "label": "Описание признаков",
      "type": "textarea",
      "id": "Field_IllnessDescription",
      "key": "illnessDescription",
      "layout": {
        "row": "Row_10"
      },
      "conditional": {
        "hide": "=illnessSigns = false"
      }
    },
    {
      "label": "Разрешение на размещение",
      "type": "select",
      "id": "Field_ApprovalStatus",
      "key": "vetApprovalStatus",
      "layout": {
        "row": "Row_11"
      },
      "validate": {
        "required": true
      },
      "values": [
        {
          "label": "Одобрено",
          "value": "approved"
        },
        {
          "label": "Одобрено с условиями",
          "value": "conditionalApproval"
        },
        {
          "label": "Отклонено",
          "value": "rejected"
        }
      ]
    },
    {
      "label": "Рекомендации",
      "type": "textarea",
      "id": "Field_VetRecommendations",
      "key": "vetRecommendations",
      "layout": {
        "row": "Row_12"
      }
    },
    {
      "label": "Имя ветеринара",
      "type": "textfield",
      "id": "Field_VetName",
      "key": "vetName",
      "layout": {
        "row": "Row_13"
      },
      "validate": {
        "required": true
      }
    }
  ],
  "type": "default",
  "id": "VetCheckForm",
  "executionPlatform": "Camunda Cloud",
  "executionPlatformVersion": "8.5.0"
}