{
  "schemaVersion": 13,
  "exporter": {
    "name": "Camunda Modeler",
    "version": "5.36.1"
  },
  "components": [
    {
      "text": "## Прогулка",
      "type": "text",
      "id": "Field_Header",
      "layout": {
        "row": "Row_0"
      }
    },
    {
      "label": "Питомец",
      "type": "textfield",
      "id": "Field_PetName",
      "key": "petName",
      "layout": {
        "row": "Row_1"
      },
      "readonly": true
    },
    {
      "label": "Время начала",
      "type": "datetime",
      "id": "Field_WalkStartTime",
      "key": "walkStartTime",
      "dateLabel": "Дата",
      "timeLabel": "Время начала",
      "layout": {
        "row": "Row_2"
      },
      "defaultValue": "=now()"
    },
    {
      "label": "Продолжительность (минут)",
      "type": "number",
      "id": "Field_WalkDuration",
      "key": "walkDuration",
      "layout": {
        "row": "Row_3"
      },
      "validate": {
        "required": true,
        "min": 5,
        "max": 120
      }
    },
    {
      "label": "Тип прогулки",
      "type": "select",
      "id": "Field_WalkType",
      "key": "walkType",
      "layout": {
        "row": "Row_4"
      },
      "values": [
        {
          "label": "Индивидуальная",
          "value": "individual"
        },
        {
          "label": "Групповая",
          "value": "group"
        }
      ]
    },
    {
      "label": "Погодные условия",
      "type": "select",
      "id": "Field_WeatherConditions",
      "key": "weatherConditions",
      "layout": {
        "row": "Row_5"
      },
      "values": [
        {
          "label": "Солнечно",
          "value": "sunny"
        },
        {
          "label": "Облачно",
          "value": "cloudy"
        },
        {
          "label": "Дождь",
          "value": "rainy"
        },
        {
          "label": "Снег",
          "value": "snowy"
        }
      ]
    },
    {
      "label": "Уровень активности",
      "type": "select",
      "id": "Field_ActivityLevel",
      "key": "activityLevel",
      "layout": {
        "row": "Row_6"
      },
      "values": [
        {
          "label": "Очень активный",
          "value": "veryActive"
        },
        {
          "label": "Активный",
          "value": "active"
        },
        {
          "label": "Умеренный",
          "value": "moderate"
        },
        {
          "label": "Малоактивный",
          "value": "lowActivity"
        }
      ]
    },
    {
      "label": "Справил нужду?",
      "type": "checklist",
      "id": "Field_ToiletActivities",
      "key": "toiletActivities",
      "layout": {
        "row": "Row_7"
      },
      "values": [
        {
          "label": "Помочился",
          "value": "urinated"
        },
        {
          "label": "Покакал",
          "value": "defecated"
        }
      ]
    },
    {
      "label": "Характер стула",
      "type": "select",
      "id": "Field_StoolCondition",
      "key": "stoolCondition",
      "layout": {
        "row": "Row_8"
      },
      "values": [
        {
          "label": "Нормальный",
          "value": "normal"
        },
        {
          "label": "Жидкий",
          "value": "loose"
        },
        {
          "label": "Твердый",
          "value": "hard"
        },
        {
          "label": "Не наблюдалось",
          "value": "notObserved"
        }
      ]
    },
    {
      "label": "Поведение во время прогулки",
      "type": "select",
      "id": "Field_WalkBehavior",
      "key": "walkBehavior",
      "layout": {
        "row": "Row_9"
      },
      "values": [
        {
          "label": "Спокойное",
          "value": "calm"
        },
        {
          "label": "Игривое",
          "value": "playful"
        },
        {
          "label": "Тревожное",
          "value": "anxious"
        },
        {
          "label": "Агрессивное",
          "value": "aggressive"
        }
      ]
    },
    {
      "label": "Примечания о прогулке",
      "type": "textarea",
      "id": "Field_WalkNotes",
      "key": "walkNotes",
      "layout": {
        "row": "Row_10"
      }
    }
  ],
  "type": "default",
  "id": "WalkForm",
  "executionPlatform": "Camunda Cloud",
  "executionPlatformVersion": "8.5.0"
}